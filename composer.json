{
    "name": "folklore/laravel-locale",
    "description": "Localization package for Laravel 5",
    "keywords": ["framework", "laravel", "localization", "locale", "translation"],
    "authors": [
        {
            "name": "Folklore",
            "email": "info@atelierfolklore.ca",
            "homepage": "http://atelierfolklore.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@atelierfolklore.ca",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "orchestra/testbench": "3.1.*|3.2.*|3.3.*|3.4.*|3.5.*|3.6.*|3.7.*|3.8.*",
        "mockery/mockery": "0.9.*|1.0.*",
        "phpunit/phpunit": "~4.0|~4.1|~5.4|~5.7|~6.0|~7.0|^7.5",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-0": {
            "Folklore\\LaravelLocale\\": "src/"
        },
        "files": [
            "src/Folklore/LaravelLocale/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Folklore\\LaravelLocale\\LocaleServiceProvider"
            ]
        }
    }
}
